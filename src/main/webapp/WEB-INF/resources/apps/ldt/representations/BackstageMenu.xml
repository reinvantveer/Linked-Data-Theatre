<!--
    DESCRIPTION
	Representation that is used when a user goes to the /backstage page.
	
-->
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:elmo="http://bp4mc2.org/elmo/def#"
         xmlns:xhtml="http://www.w3.org/1999/xhtml/vocab#"
		 xmlns:dcterms="http://purl.org/dc/terms/"
>
	<rdf:Description rdf:about="http://bp4mc2.org/elmo/def#BackstageRepresentation_Header">
		<elmo:appearance rdf:resource="http://bp4mc2.org/elmo/def#HeaderAppearance"/>
		<elmo:data rdf:nodeID="header"/>
	</rdf:Description>
	<rdf:Description rdf:nodeID="header">
		<elmo:html rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#HTML"><![CDATA[<img src='/images/ldt-logo.png'/>]]></elmo:html>
	</rdf:Description>
	<rdf:Description rdf:about="http://bp4mc2.org/elmo/def#BackstageRepresentation_Menu">
		<elmo:appearance rdf:resource="http://bp4mc2.org/elmo/def#NavbarAppearance"/>
		<elmo:data rdf:nodeID="menu"/>
	</rdf:Description>
	<rdf:Description rdf:nodeID="menu">
		<dcterms:title>Backstage</dcterms:title>
		<elmo:data rdf:nodeID="menu-item1"/>
		<xhtml:link>../backstage</xhtml:link>
	</rdf:Description>
